{
  "version": 2,
  "builds": [
    {
      "src": "api/[...backend].ts",
      "use": "@vercel/node"
    },
    {
      "src": "packages/frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...backend].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/frontend/dist/$1"
    }
  ]
}
